{
  "name": "vacanciessearch",
  "version": "1.0.0",
  "description": "vacancies search",
  "keywords": [
    "vacancies",
    "job",
    "search",
    "react",
    "nodejs"
  ],
  "license": "ISC",
  "author": "Mikhail Pisemsky <mikhailpisemsky@yandex.ru>",
  "type": "commonjs",
  "main": "app.js",
  "scripts": {
    "migrate:init": "npx sequelize-cli db:migrate --migration-name init-database",
    "migrate:fix": "npx sequelize-cli db:migrate --migration-name fix-students-application",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "migrate:create": "npx sequelize-cli migration:generate --name",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "file-system": "^2.2.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "path": "^0.12.7",
    "pg": "^8.16.0",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10"
  }
}
